{
  "name": "nest-config-center",
  "version": "1.0.0",
  "description": "Nest TypeScript starter repository",
  "license": "MIT",
  "private": "true",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build && yarn build:client",
    "build:client": "cross-env NODE_ENV=production webpack --config webpack.prod.js",
    "format": "prettier --write \"server/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "cross-env NODE_ENV=development  nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint '{server,apps,libs,test}/**/*.ts' --fix",
    "test": "jest  --detectOpenHandles",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "echo 'No e2e tests implemented yet.'"
  },
  "dependencies": {
    "@nestjs/common": "8.2.3",
    "@nestjs/config": "^1.1.6",
    "@nestjs/core": "8.2.3",
    "@nestjs/platform-express": "8.2.3",
    "@nestjs/typeorm": "8.0.2",
    "@reduxjs/toolkit": "^1.7.1",
    "antd": "^4.18.2",
    "axios": "^0.24.0",
    "mysql": "2.18.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.6",
    "react-router-dom": "^6.2.1",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "7.4.0",
    "typeorm": "0.2.41",
    "use-immer": "^0.6.0"
  },
  "devDependencies": {
    "@nestjs/cli": "8.1.5",
    "@nestjs/schematics": "8.0.5",
    "@nestjs/testing": "8.2.3",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.4",
    "@types/axios": "^0.14.0",
    "@types/express": "4.17.13",
    "@types/jest": "27.0.3",
    "@types/node": "16.11.12",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/react-redux": "^7.1.21",
    "@types/react-router-dom": "^5.3.2",
    "@types/styled-components": "^5.1.19",
    "@types/supertest": "2.0.11",
    "@types/ws": "7.4.7",
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "@typescript-eslint/parser": "4.33.0",
    "babel-plugin-import": "^1.13.3",
    "connect-history-api-fallback": "^1.6.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.5.1",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.25.3",
    "jest": "27.3.1",
    "prettier": "2.5.1",
    "react-hot-loader": "^4.13.0",
    "react-refresh": "^0.11.0",
    "supertest": "6.1.6",
    "ts-jest": "27.0.7",
    "ts-loader": "9.2.6",
    "ts-node": "10.4.0",
    "tsconfig-paths": "3.11.0",
    "typescript": "4.3.5",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-middleware": "^5.3.0",
    "webpack-hot-middleware": "^2.25.1"
  }
}
