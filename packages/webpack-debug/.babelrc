{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "chrome": "93"
        }
      }
    ],
    [
      "@babel/preset-react",
      {
        "runtime": "automatic"
      }
    ],
    "@babel/preset-typescript"
  ],
  "plugins": [
    ["@babel/plugin-transform-runtime", { "corejs": 3, "proposals": true }]
  ]
}
